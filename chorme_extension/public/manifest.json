{
  "manifest_version": 3,
  "name": "Login Tab Extension",
  "version": "1.0",
  "description": "An extension that opens a new tab with a login page.",

  "permissions": ["tabs"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "host_permissions":[
    "https://*/*",
    "http://*/"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
